<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="log4net" type ="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
	</configSections>

	<log4net>
		<logger name="ZoneCalculation">
			<level value="INFO" />
			<appender-ref ref="Test1" />
		</logger>
		<appender name="Test1" type="log4net.Appender.FileAppender">
			<layout name="PatternLayout" type="log4net.Layout.PatternLayout">
				<ConversionPattern value="%date{yyyy-MM-dd HH:mm:ss} [%class] [%level] [%method] - %message%newline"/>
			</layout>
			<File value="C:\Users\IvannikovRV\Documents\Logger\Test1.log"/>
			<Name value= "ZoneCalculation"/>
			<AppendToFile value ="True"/>
		</appender>

	</log4net>
</configuration>